{
  "name": "react-youtube-sync-server",
  "version": "0.1.0",
  "description": "A socket.io server proving synchronisation of a youtube video player between multiple clients",
  "main": "./src/server.js",
  "scripts": {
    "start": "nodemon --require reify ./src/main.js",
    "lint": "esw *.js src --color",
    "lint:init": "eslint --init",
    "lint:watch": "npm run lint -- --watch",
    "build": "babel ./src/server.js -o ./src/herokuserver.js"
  },
  "dependencies": {
    "babel-cli": "6.22.2",
    "babel-preset-es2015": "6.22.0",
    "debug": "2.6.8",
    "lodash": "4.17.4",
    "node-cache": "4.1.1",
    "reify": "0.4.4",
    "socket.io": "2.0.3"
  },
  "devDependencies": {
    "eslint": "3.11.1",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-standard": "2.0.1",
    "eslint-watch": "2.1.14",
    "nodemon": "1.11.0"
  },
  "engines": {
    "node": ">=7.x",
    "npm": ">=3.x"
  }
}
